{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "NestJS debug",
            "type": "node",
            "request": "launch",
            "args": [
                "${workspaceFolder}/dist/main.js"
            ],
            "runtimeArgs": [
                "--nolazy",
                "-r",
                "ts-node/register",
                "-r",
                "tsconfig-paths/register"
            ],
            "sourceMaps": true,
            "cwd": "${workspaceRoot}",
            "protocol": "inspector",
            // # "env": {"NODE_ENV":"development"},
            "console": "integratedTerminal"
        }
    ]
}